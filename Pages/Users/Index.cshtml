@page
@model SupermarketWEB.Pages.Users.IndexModel
@{
	ViewData["Title"] = "index Users";
}

<h1>Index Users</h1>
<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Users[0].Id)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Users[0].Name)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Users[0].PasswordHash)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Users[0].Role)
			</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.Users)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Id)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Name)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.PasswordHash)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Role)
				</td>
				<td>
					<span></span>
				</td>
			</tr>
		}
	</tbody>
</table>